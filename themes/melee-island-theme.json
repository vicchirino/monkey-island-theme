  "tokenColors": [
    {
      "scope": [],
      "settings": {
        "foreground": "#a6a6a6"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#d2d2d27a"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder", "source"],
      "settings": {
        "foreground": "#a6a6a6"
      }
    },
    {
      "name": "Colors",
      "scope": [
